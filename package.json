{
  "name": "emitter",
  "version": "1.0.0",
  "description": "Simple async emitter",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf dist/ coverage/ .nyc_output/ node_modules/.cache/",
    "format": "prettier-standard {src,test}/*.js",
    "lint": "standard {src,test}/*.js",
    "test": "nyc ava",
    "coverage": "npm run test && nyc report --reporter=html && sirv coverage/ -p 8000",
    "build": "rollup -c rollup.config.js"
  },
  "repository": {
    "type": "git",
    "url": "/nas/config/git/emitter.git"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "ava": "^1.4.1",
    "esm": "^3.2.25",
    "nyc": "^14.1.1",
    "prettier-standard": "^9.1.1",
    "rollup": "^1.12.3",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-terser": "^4.0.4",
    "sirv-cli": "^0.4.2",
    "snazzy": "^8.0.0",
    "standard": "^12.0.1"
  },
  "ava": {
    "require": [
      "esm"
    ]
  }
}
